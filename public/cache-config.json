{
  "version": "1.0.0",
  "cacheStrategy": {
    "staticAssets": {
      "maxAge": 31536000,
      "immutable": true,
      "include": ["*.js", "*.css", "*.woff", "*.woff2", "*.ttf", "*.eot"]
    },
    "images": {
      "maxAge": 15552000,
      "immutable": false,
      "include": ["*.png", "*.jpg", "*.jpeg", "*.gif", "*.svg", "*.webp"]
    },
    "documents": {
      "maxAge": 2592000,
      "immutable": false,
      "include": ["*.pdf", "*.doc", "*.docx"]
    },
    "api": {
      "maxAge": 300,
      "immutable": false,
      "strategy": "networkFirst"
    },
    "html": {
      "maxAge": 0,
      "immutable": false,
      "strategy": "networkFirst"
    }
  },
  "preload": {
    "critical": [
      "/static/css/main.css",
      "/static/js/bundle.js",
      "https://fonts.googleapis.com/css2?family=Inter&family=Open+Sans&display=swap"
    ],
    "high": [
      "https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.6.0/slick.min.css",
      "https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.6.0/slick-theme.min.css"
    ],
    "medium": [
      "https://flagcdn.com/w80/au.png",
      "https://flagcdn.com/w80/ca.png",
      "https://flagcdn.com/w80/gb.png",
      "https://flagcdn.com/w80/us.png"
    ]
  },
  "compression": {
    "enabled": true,
    "algorithms": ["gzip", "brotli"],
    "minSize": 1024
  },
  "cdn": {
    "enabled": true,
    "domains": [
      "cdnjs.cloudflare.com",
      "fonts.googleapis.com",
      "fonts.gstatic.com",
      "flagcdn.com"
    ]
  }
}
